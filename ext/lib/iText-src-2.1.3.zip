        lingrad.addColorStop(0.5, 'rgba(135,135,135,0.5)');
        lingrad.addColorStop(1, 'rgba(255,255,255,0)');
        ctx.fillStyle = lingrad;
        ctx.fillRect(0,0,1000,150);
	}

	if (document.getElementById('high')){
        var ctx = document.getElementById('high').getContext('2d');
        var lingrad = ctx.createLinearGradient(0,0,0,50);
        lingrad.addColorStop(0, 'rgba(255,255,255,0)');
        lingrad.addColorStop(0.1, 'rgba(255,255,255,0.2)');
        lingrad.addColorStop(1, 'rgba(255,255,255,0)');
        ctx.fillStyle = lingrad;
        ctx.fillRect(0,0,1000,50);
	}
        
